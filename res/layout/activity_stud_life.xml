<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    android:id="@+id/coordinatorInStudLife"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameForStudLife"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </FrameLayout>

    <include layout="@layout/bottom_sheet_for_msa"/>

    <RelativeLayout
        android:id="@+id/controlForFAB"
        android:layout_gravity="right|bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabOnStudLife"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_anchorGravity="right|end"
        app:layout_anchor="@+id/viewForMSA"
        android:src="@drawable/ic_more_horiz_white_24dp"/>

    </RelativeLayout>

</android.support.design.widget.CoordinatorLayout>
